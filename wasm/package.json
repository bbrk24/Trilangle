{
    "dependencies": {
        "coffeescript": "^2.7.0",
        "sass": "^1.64.1",
        "terser": "^5.19.2"
    },
    "devDependencies": {
        "@coffeelint/cli": "^5.2.11",
        "clang-format": "^1.8.0",
        "engine-check": "git@github.com:bbrk24/engine-check.git#e2eb23075fc166873ceab995cc4c9b206733a9b5",
        "express": "^4.18.0",
        "stylelint": "^15.10.2",
        "stylelint-config-standard-scss": "^11.0.0"
